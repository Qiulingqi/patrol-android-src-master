<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape>
            <gradient
                android:angle="270"
                android:centerColor="@color/color_gray_8"
                android:endColor="@color/color_gray_9"
                android:startColor="@color/color_gray_10" />
        </shape>

    </item>
    <item
        android:bottom="2dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/tran_blue" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="5dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
        </shape>
    </item>


</layer-list>